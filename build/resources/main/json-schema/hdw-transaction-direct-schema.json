{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "definitions": {
    "positiveInteger": {
      "type": "integer",
      "exclusiveMinimum": 0,
      "examples": [
        109823,
        2312234,
        122
      ]
    },
    "positiveNumber": {
      "type": "number",
      "exclusiveMinimum": 0.0,
      "examples": [
        34.234,
        0.001,
        122.00,
        12
      ]
    },
    "nonEmptyString": {
      "type": "string",
      "minLength": 1,
      "examples": [
        "Any String",
        "   "
      ]
    },
    "dateTime": {
      "type": "string",
      "examples": [
        "2018-08-10T09:30:00.000"
      ],
      "pattern": "^([0-9]{4})-([0-9]{2})-([0-9]{2})T([0-9]{2}):([0-9]{2}):([0-9]{2})(.[0-9]{3,6})?$"
    },
    "emailId": {
      "type": "string",
      "format": "email",
      "examples": [
        "john.snow134@mail.com"
      ]
    }
  },
  "type": "object",
  "properties": {
    "attributes": {
      "type": "object",
      "properties": {
        "schemaUrl": {
          "type": "string",
          "$ref": "#/definitions/nonEmptyString"
        },
        "contract_version": {
          "type": "string",
          "examples": ["0.1"],
          "$ref": "#/definitions/nonEmptyString"
        },
        "source": {
          "type": "string",
          "$ref": "#/definitions/nonEmptyString"
        },
        "event_type": {
          "type": "string",
          "$ref": "#/definitions/nonEmptyString"
        },
        "platform": {
          "type": "string",
          "$ref": "#/definitions/nonEmptyString"
        },
        "dc_number": {
          "type": "string",
          "$ref": "#/definitions/nonEmptyString",
          "minLength": 4
        },
        "action": {
          "type": "string",
          "$ref": "#/definitions/nonEmptyString"
        },
        "activity": {
          "type": "string",
          "$ref": "#/definitions/nonEmptyString"
        },
        "publish_timestamp": {
          "type": "string",
          "$ref": "#/definitions/dateTime"
        },
        "trace_id": {
          "type": "string",
          "$ref": "#/definitions/nonEmptyString"
        },
        "task_id": {
          "type": "string"
        }
      },
      "required": [
        "schemaUrl",
        "contract_version",
        "source",
        "event_type",
        "platform",
        "dc_number",
        "action",
        "activity",
        "publish_timestamp",
        "trace_id"
      ]
    },
    "data": {
      "type": "object",
      "properties": {
        "user_id": {
          "type": "string",
          "$ref": "#/definitions/emailId"
        },
        "ldap_id": {
          "type": "string",
          "$ref": "#/definitions/nonEmptyString"
        },
        "transaction_id": {
          "type": "string",
          "$ref": "#/definitions/nonEmptyString"
        },
        "vehicle": {
          "type": "object",
          "properties": {
            "assigned_vehicle": {
              "type": "string"
            },
            "vehicle_id": {
              "type": "string"
            }
          },
          "required": [
            "assigned_vehicle",
            "vehicle_id"
          ]
        },
        "lpns": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "parent_lpn_id": {
                "type": "string"
              },
              "lpn_number": {
                "type": "string",
                "$ref": "#/definitions/nonEmptyString"
              },
              "container_type": {
                "type": "string"
              },
              "locations": {
                "type": "array",
                "items": {
                  "type": "object",
                  "properties": {
                    "transaction_timestamp": {
                      "type": "string",
                      "$ref": "#/definitions/dateTime"
                    },
                    "start_location": {
                      "type": "string",
                      "$ref": "#/definitions/nonEmptyString"
                    },
                    "end_location": {
                      "type": "string",
                      "$ref": "#/definitions/nonEmptyString"
                    },
                    "start_zone": {
                      "type": "string",
                      "$ref": "#/definitions/nonEmptyString"
                    },
                    "end_zone": {
                      "type": "string",
                      "$ref": "#/definitions/nonEmptyString"
                    },
                    "start_location_type": {
                      "type": "string",
                      "$ref": "#/definitions/nonEmptyString"
                    },
                    "end_location_type": {
                      "type": "string",
                      "$ref": "#/definitions/nonEmptyString"
                    },
                    "pick_area": {
                      "type": "string"
                    },
                    "put_area": {
                      "type": "string"
                    },
                    "skus": {
                      "type": "array",
                      "items": {
                        "type": "object",
                        "properties": {
                          "sku_number": {
                            "type": "string",
                            "$ref": "#/definitions/nonEmptyString"
                          },
                          "build_id": {
                            "type": "string",
                            "$ref": "#/definitions/nonEmptyString"
                          },
                          "sku_description": {
                            "type": "string"
                          },
                          "department": {
                            "type": "string",
                            "$ref": "#/definitions/nonEmptyString"
                          },
                          "sku_class": {
                            "type": "string",
                            "$ref": "#/definitions/nonEmptyString"
                          },
                          "sku_sub_class": {
                            "type": "string",
                            "$ref": "#/definitions/nonEmptyString"
                          },
                          "special_handling": {
                            "type": "string"
                          },
                          "build_on_method": {
                            "type": "string"
                          },
                          "secure_method": {
                            "type": "string"
                          },
                          "unload_type": {
                            "type": "string"
                          }
                        },
                        "required": [
                          "sku_number",
                          "build_id",
                          "department",
                          "sku_class",
                          "sku_sub_class"
                        ]
                      }
                    },
                    "location_uom": {
                      "type": "string",
                      "$ref": "#/definitions/nonEmptyString"
                    },
                    "location_qty": {
                      "type": "string",
                      "$ref": "#/definitions/nonEmptyString"
                    },
                    "uom_qty": {
                      "type": "string",
                      "$ref": "#/definitions/nonEmptyString"
                    },
                    "inbound_outbound_indicator": {
                      "type": "string"
                    },
                    "reason_code": {
                      "type": "string"
                    },
                    "order_category": {
                      "type": "string"
                    },
                    "shipment_number": {
                      "type": "string"
                    },
                    "shipment_type_id": {
                      "type": "string"
                    },
                    "shipment_route": {
                      "type": "string"
                    },
                    "shipment_stop": {
                      "type": "string"
                    },
                    "store_number": {
                      "type": "string"
                    },
                    "service_type": {
                      "type": "string"
                    },
                    "vendor_number": {
                      "type": "string"
                    },
                    "trailer_number": {
                      "type": "string"
                    },
                    "rail_car_number": {
                      "type": "string"
                    },
                    "scac": {
                      "type": "string"
                    },
                    "lpn_status": {
                      "type": "string"
                    },
                    "shipment_lpn_error_type": {
                      "type": "string"
                    },
                    "mhe_loaded": {
                      "type": "string"
                    }
                  },
                  "required": [
                    "transaction_timestamp",
                    "start_location",
                    "end_location",
                    "start_zone",
                    "end_zone",
                    "start_location_type",
                    "end_location_type",
                    "skus",
                    "location_uom",
                    "location_qty",
                    "uom_qty",
                    "inbound_outbound_indicator"
                  ]
                }
              }
            },
            "required": [
              "lpn_number",
              "locations"
            ]
          }
        },
        "builds": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "build_id": {
                "type": "string",
                "$ref": "#/definitions/nonEmptyString"
              },
              "sku": {
                "type": "string",
                "$ref": "#/definitions/nonEmptyString"
              },
              "package_hierarchy": {
                "type": "array",
                "items": {
                  "type": "object",
                  "properties": {
                    "depth": {
                      "type": "number",
                      "$ref": "#/definitions/positiveNumber"
                    },
                    "weight_uom": {
                      "type": "string",
                      "$ref": "#/definitions/nonEmptyString"
                    },
                    "size_uom": {
                      "type": "string",
                      "$ref": "#/definitions/nonEmptyString"
                    },
                    "width": {
                      "type": "number",
                      "$ref": "#/definitions/positiveNumber"
                    },
                    "weight": {
                      "type": "number",
                      "$ref": "#/definitions/positiveNumber"
                    },
                    "height": {
                      "type": "number",
                      "$ref": "#/definitions/positiveNumber"
                    },
                    "package_unit_qty": {
                      "type": "string",
                      "$ref": "#/definitions/nonEmptyString"
                    },
                    "package_each_qty": {
                      "type": "string",
                      "$ref": "#/definitions/nonEmptyString"
                    },
                    "hierarchy_level_code": {
                      "type": "string",
                      "$ref": "#/definitions/nonEmptyString"
                    }
                  },
                  "required": [
                    "depth",
                    "weight_uom",
                    "size_uom",
                    "width",
                    "weight",
                    "height",
                    "package_unit_qty",
                    "package_each_qty",
                    "hierarchy_level_code"
                  ]
                }
              }
            },
            "required": [
              "build_id",
              "sku",
              "package_hierarchy"
            ]
          }
        }
      },
      "required": [
        "user_id",
        "ldap_id",
        "transaction_id",
        "lpns",
        "builds"
      ]
    }
  },
  "required": [
    "attributes",
    "data"
  ]
}